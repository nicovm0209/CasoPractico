<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CasoEstudioDos</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CasoEstudioDos.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CasoEstudioDos</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item" style="margin-left: 50px; text-align: center;">
                            <img src="https://cdn1.iconfinder.com/data/icons/jumpicon-basic-ui-glyph-1/32/-_Home-House--256.png" style="width: 30px; height: 30px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item" style="margin-left: 50px; text-align: center;">
                            <img src="https://cdn2.iconfinder.com/data/icons/ios-7-icons/50/user_male4-256.png" style="width: 30px; height: 30px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Sign In</a>
                        </li>
                        <li class="nav-item" style="margin-left: 50px; text-align: center;">
                            <img src="https://cdn4.iconfinder.com/data/icons/evil-icons-user-interface/64/plus-256.png" style="width: 30px; height: 30px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Sign Up</a>
                        </li>
                        <li class="nav-item" style="margin-left: 50px; text-align: center;">
                            <img src="https://cdn2.iconfinder.com/data/icons/ios-7-icons/50/user_male2-512.png" style="width: 30px; height: 30px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Profile</a>
                        </li>
                        <li class="nav-item" style="margin-left: 50px; text-align: center;">
                            <img src="https://cdn3.iconfinder.com/data/icons/simple-files-1/128/Lock-256.png" style="width: 30px; height: 30px;">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - CasoEstudioDos - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
